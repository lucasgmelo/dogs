n\nconst LoginForm = () => {\n  const username = useForm();\n  const password = useForm();\n\n  const { userLogin, error, loading, setLoading } = React.useContext(\n    UserContext\n  );\n\n  async function handleSubmit(event) {\n    event.preventDefault();\n\n    if (username.validate() && password.validate()) {\n      userLogin(username.value, password.value);\n    }\n  }\n\n  return (\n    <section className=\"animeLeft\">\n      <Title>Login</Title>\n\n      <form className={style.form} onSubmit={handleSubmit}>\n        <Input label=\"Usuário\" type=\"text\" name=\"username\" {...username} />\n        <Input label=\"Senha\" type=\"password\" name=\"password\" {...password} />\n \n        {loading ? (\n          <Button loading={true} childrens=\"Carregando...\" />\n        ) : (\n          <Button loading={false} childrens=\"Entrar\" />\n        )}\n\n        <Helper error={error} />\n      </form>\n      <Link className={style.lost} to=\"/login/lost\">Perdeu a senha?</Link>\n      <div className={style.register}>\n        <Subtitle>Cadastre-se</Subtitle>\n        <p>Ainda não possui conta? Cadastre-se no site.</p>\n      </div>\n      \n      <Link to=\"/login/create\" className={style.button}>Cadastro</Link>\n    </section>\n  );\n};\n\nexport default LoginForm;\n","/Users/lucasmelo/dogs/src/Components/Login/Login.js",[],"/Users/lucasmelo/dogs/src/App.js",[],"/Users/lucasmelo/dogs/src/Components/Login/LoginPasswordReset.js",[],["125","126"],"/Users/lucasmelo/dogs/src/Components/Header.js",[],"/Users/lucasmelo/dogs/src/Components/Footer.js",[],"/Users/lucasmelo/dogs/src/Components/Forms/Input/Input.js",[],"/Users/lucasmelo/dogs/src/Components/Forms/Button/Button.js",["127"],"/Users/lucasmelo/dogs/src/Components/Forms/Button/ButtonStyles.js",[],"/Users/lucasmelo/dogs/src/Components/MainStyles.js",[],"/Users/lucasmelo/dogs/src/Components/Forms/Input/InputStyles.js",[],"/Users/lucasmelo/dogs/src/Hooks/useForm.js",["128"],"import React, {useState} from 'react'\n\nconst types = {\n    email: {\n        regex: /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/,\n        message: 'Preencha um email válido.',\n    },\n}\n\nconst useForm = (type) => {\n\n    const [value, setValue] = useState('');\n    const [error, setError] = useState(null);\n\n    function validate(value){\n        if (type === false) return true;\n        if (value.length === 0) {\n            setError('Preencha um valor.')\n            return false;\n        } else if (types[type] && !types[type].regex.test(value)) {\n            setError(types[type].message);\n            return false;\n        } else {\n            setError(null);\n            return true;\n        }\n\n    }\n\n    function onChange({target}) {\n        if(error) validate(target.value);\n        setValue(target.value);\n    }\n\n    return {\n        value,\n        setValue,\n        onChange,\n        error,\n        validate: () => validate(value),\n        onBlur: () => validate(value),\n    }\n}\n\nexport default useForm\n","/Users/lucasmelo/dogs/src/Components/Login/LoginCreate.js",[],"/Users/lucasmelo/dogs/src/UserContext.js",[],"/Users/lucasmelo/dogs/src/Components/Helper/Helper.js",[],"/Users/lucasmelo/dogs/src/services/api.js",[],"/Users/lucasmelo/dogs/src/services/USER_POST.js",[],"/Users/lucasmelo/dogs/src/services/TOKEN_POST.js",[],"/Users/lucasmelo/dogs/src/Hooks/useFetch.js",["129"],"/Users/lucasmelo/dogs/src/Components/Helper/ProtectedRouter.js",[],"/Users/lucasmelo/dogs/src/Components/User/User.js",[],"/Users/lucasmelo/dogs/src/Components/Feed/Feed.js",[],"/Users/lucasmelo/dogs/src/Components/User/UserPhotoPost.js",[],"/Users/lucasmelo/dogs/src/Components/User/UserHeader.js",[],"/Users/lucasmelo/dogs/src/Components/User/UserHeaderNav.js",["130"],"/Users/lucasmelo/dogs/src/services/URL.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy